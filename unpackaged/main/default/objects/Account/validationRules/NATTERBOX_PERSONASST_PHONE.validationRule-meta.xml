<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NATTERBOX_PERSONASST_PHONE</fullName>
    <active>true</active>
    <description>Check person assistant phone number is valid for use in Natterbox</description>
    <errorConditionFormula>NOT(OR(ISBLANK(PersonAssistantPhone), REGEX(PersonAssistantPhone,&quot;^\\+[1-9][0-9]*&quot;)))</errorConditionFormula>
    <errorDisplayField>PersonAssistantPhone</errorDisplayField>
    <errorMessage>INVALID TELEPHONE FORMAT</errorMessage>
</ValidationRule>
