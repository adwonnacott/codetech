<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NATTERBOX_HOMENUM</fullName>
    <active>true</active>
    <description>Check home phone number is valid for Natterbox</description>
    <errorConditionFormula>NOT(OR(ISBLANK(  HomePhone  ), REGEX(  HomePhone  ,&quot;^\\+[1-9][0-9]*&quot;)))</errorConditionFormula>
    <errorDisplayField>HomePhone</errorDisplayField>
    <errorMessage>INVALID PHONE NUMBER</errorMessage>
</ValidationRule>
