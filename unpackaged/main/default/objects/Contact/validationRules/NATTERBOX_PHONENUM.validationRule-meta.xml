<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NATTERBOX_PHONENUM</fullName>
    <active>true</active>
    <description>Check other phone number is valid for Natterbox</description>
    <errorConditionFormula>NOT(OR(ISBLANK( Phone ), REGEX( Phone ,&quot;^\\+[1-9][0-9]*&quot;)))</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>INVALID PHONE NUMBER</errorMessage>
</ValidationRule>
