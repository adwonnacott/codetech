<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NATTERBOX_MOBILE</fullName>
    <active>true</active>
    <description>Check mobile phone number is valid for natterbox</description>
    <errorConditionFormula>NOT(OR(ISBLANK( MobilePhone ), REGEX( MobilePhone ,&quot;^\\+[1-9][0-9]*&quot;)))</errorConditionFormula>
    <errorDisplayField>MobilePhone</errorDisplayField>
    <errorMessage>INVALID TELEPHONE NUMBER</errorMessage>
</ValidationRule>
