<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>nbavs__Day_Of_Week_Short__c</fullName>
    <deprecated>false</deprecated>
    <description>Short day of week when the availability state changed</description>
    <externalId>false</externalId>
    <formula>CASE( 
MOD(DATEVALUE( nbavs__Time__c ) - DATE(1900, 1, 7), 7), 
0, &quot;Sun&quot;, 
1, &quot;Mon&quot;, 
2, &quot;Tue&quot;, 
3, &quot;Wed&quot;, 
4, &quot;Thu&quot;, 
5, &quot;Fri&quot;, 
6, &quot;Sat&quot;, &quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Short day of week when the availability state changed</inlineHelpText>
    <label>Day Of Week Short</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
