<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>nbavs__MinMaxQuerySize</fullName>
    <active>true</active>
    <errorConditionFormula>OR(nbavs__QueryLimit__c &lt; 5, nbavs__QueryLimit__c &gt; 2000)</errorConditionFormula>
    <errorMessage>The max active dial items setting must be between 5 and 2000</errorMessage>
</ValidationRule>
